[Script]
哔哩哔哩Cookie(APP) = type=http-request,pattern=^https?:\/\/app\.bilibili\.com\/x\/resource\/fingerprint\?,requires-body=0,script-path=https://raw.githubusercontent.com/myouhi/Surge/master/tasks/cookie/BiliBili.js,script-update-interval=0
哔哩哔哩Cookie(网页) = type=http-request,pattern=^https?:\/\/m.bilibili.com/$,requires-body=0,script-path=https://raw.githubusercontent.com/myouhi/Surge/master/tasks/cookie/BiliBili.js,script-update-interval=0
喜马拉雅Cookie = type=http-request,pattern=^https?:\/\/xmc\.ximalaya\.com\/xmlymain-login-web\/login\/,requires-body=0,script-path=https://raw.githubusercontent.com/myouhi/Surge/master/tasks/cookie/xmly.js,script-update-interval=0
WPS_PC获取Cookie = type=http-request,pattern=^https:\/\/(vip|account)(userinfo|\.wps\.cn\/p\/auth\/check)$,requires-body=0,script-path=https://raw.githubusercontent.com/myouhi/Surge/master/tasks/cookie/wps.js,script-update-interval=0
吾爱破解获取Cookie = type=http-request,pattern=^https:\/\/www\.52pojie\.cn\/home\.php\?,requires-body=0,script-path=https://raw.githubusercontent.com/myouhi/Surge/master/tasks/cookie/52pojie.js,script-update-interval=0

[MITM]
hostname = %APPEND% app.bilibili.com, m.bilibili.com, xmc.ximalaya.com, *.wps.cn, www.52pojie.cn
